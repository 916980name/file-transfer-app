          ".fwdred",
          ".gff3",
          ".hl7v2",
          ".javascript",
          ".markdown",
          ".mizar",
          ".n3",
          ".parameters",
          ".parityfec",
          ".plain",
          ".raptorfec",
          ".RED",
          ".richtext",
          ".rtf",
          ".SGML",
          ".shaclc",
          ".shex",
          ".spdx",
          ".strings",
          ".t140",
          ".tab",
          ".troff",
          ".turtle",
          ".ulpfec",
          ".uri",
          ".vcard",
          ".vtt",
          ".wgsl"
        ],
        "video/*": [
          ".webm",
          ".avi",
          ".mov",
          ".3gpp",
          ".3gpp2",
          ".AV1",
          ".BMPEG",
          ".BT656",
          ".CelB",
          ".DV",
          ".encaprtp",
          ".evc",
          ".example",
          ".FFV1",
          ".flexfec",
          ".jxsv",
          ".matroska",
          ".mj2",
